--------------------
debut
   afficher "Saisir age : "
   lire age

   //Selection simple Si
   si age > 5 alors
      age <- age + 1
   fin si

   //Selection Si Sinon
   si		age > 10 alors
      age <- age - 4
   sinon
      age <- age - 3
      ecrire age
   fin si

   //Selection multiple Si Sinon si Sinon
   si age > 12 alors

      //selection interne
      si age = 14 alors
         afficher "WOW"
      sinon
         afficher "OK"
      fin   si

   sinon   si age = 15   alors
      ecrire "BROUM"
   sinon si age = 16		alors
      afficher "Si"
   sinon
      ecrire "Fin"
   fin si

fin
