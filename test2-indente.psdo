//L'indentation et le formatage attendus du fichier test2.psdo - en majuscules :

--------------------
DEBUT
   AFFICHER "Saisir age : "
   LIRE age

   //Selection simple Si
   SI age > 5 ALORS
      age <- age + 1
   FIN SI

   //Selection Si Sinon
   SI		age > 10 ALORS
      age <- age - 4
   SINON
      age <- age - 3
      ECRIRE age
   FIN SI

   //Selection multiple Si Sinon si Sinon
   SI age > 12 ALORS

      //selection interne
      SI age = 14 ALORS
         AFFICHER "WOW"
      SINON
         AFFICHER "OK"
      FIN   SI

   SINON   SI age = 15   ALORS
      ECRIRE "BROUM"
   SINON SI age = 16		ALORS
      AFFICHER "Si"
   SINON
      ECRIRE "Fin"
   FIN SI

FIN
--------------------

